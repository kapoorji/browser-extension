{
  "manifest_version": 2,

  "name": "Chrome Extension",
  "description": "This extension modifies DOM.",
  "version": "1.0",
  "content_scripts": [{
    
    "js": ["assets/js/content.js"],
    "matches": ["<all_urls>"]
  }],
  "background": {
    "scripts": ["assets/js/background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
